# Uncomment the next line to define a global platform for your project
 platform :ios, '10.0'

target 'Tempo' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Tempo
  pod 'GoogleMaps'
  pod 'GooglePlaces'
  pod 'Gifu'
  pod 'SwiftyJSON'
  pod 'Alamofire', '~> 4.8'
  pod 'SVProgressHUD'
  pod 'Stripe'

end
